# Fire Detection Service Configuration
# Copy this file to .env and update with your values

# Service Port
PORT=8002

# Laravel Backend API
BACKEND_API_URL=http://localhost:8000/api/v1

# Model Configuration
MODEL_PATH=./models
CONFIDENCE_THRESHOLD=0.6

# Camera Configuration (if running standalone)
CAMERA_ID=0
FLOOR_ID=1
ROOM_LOCATION=Room 101

# Detection Parameters
DETECTION_INTERVAL=1.0
COOLDOWN_SECONDS=30
